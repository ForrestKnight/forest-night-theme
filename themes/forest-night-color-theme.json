  "tokenColors": [
    // Comments
    {
      "name": "Comment",
      "scope": [
        "comment",
        "punctuation.definition.comment",
        "comment.line",
        "comment.block"
      ],
      "settings": {
        "fontStyle": "italic",
        "foreground": "#4a5568"
      }
    },
    {
      "name": "Comment Keywords",
      "scope": [
        "comment.line.double-slash keyword",
        "comment.block keyword"
      ],
      "settings": {
        "fontStyle": "italic bold",
        "foreground": "#6b7280"
      }
    },

    // Variables and Parameters
    {
      "name": "Variables",
      "scope": [
        "variable",
        "variable.other",
        "variable.parameter",
        "variable.language",
        "variable.other.readwrite"
      ],
      "settings": {
        "foreground": "#c9d1d9"
      }
    },
    {
      "name": "Variable Destructuring",
      "scope": [
        "variable.other.destructuring"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },

    // Functions and Methods
    {
      "name": "Functions",
      "scope": [
        "entity.name.function",
        "meta.function-call entity.name.function",
        "meta.function-call variable.function",
        "support.function",
        "keyword.other.special-method",
        "meta.method-call entity.name.function"
      ],
      "settings": {
        "foreground": "#9B59B6"
      }
    },
    {
      "name": "Built-in Functions",
      "scope": [
        "support.function.builtin",
        "support.function.construct",
        "support.function.magic"
      ],
      "settings": {
        "foreground": "#9B59B6",
        "fontStyle": "bold"
      }
    },

    // Strings and Characters
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.quoted.single",
        "string.quoted.double",
        "string.quoted.triple",
        "punctuation.definition.string"
      ],
      "settings": {
        "foreground": "#4ECDC4"
      }
    },
    {
      "name": "Template Strings",
      "scope": [
        "string.template",
        "string.quoted.template"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "String Escape Characters",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#F39C12",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Regular Expressions",
      "scope": [
        "string.regexp",
        "constant.other.character-class.regexp",
        "constant.character.escape.regexp"
      ],
      "settings": {
        "foreground": "#c78a7a"
      }
    },

    // Properties and Object Keys
    {
      "name": "Object Properties",
      "scope": [
        "variable.other.property",
        "variable.other.object.property",
        "variable.other.property.js",
        "meta.object-literal.key",
        "support.type.property-name"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },
    {
      "name": "JSON Keys",
      "scope": [
        "support.type.property-name.json",
        "meta.structure.dictionary.key.json string.quoted.double.json"
      ],
      "settings": {
        "foreground": "#66D9EF"
      }
    },

    // Keywords and Control Flow
    {
      "name": "Keywords",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator.new",
        "keyword.other.special-method",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#F39C12"
      }
    },
    {
      "name": "Control Flow Keywords",
      "scope": [
        "keyword.control.flow",
        "keyword.control.conditional",
        "keyword.control.loop"
